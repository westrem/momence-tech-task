{
  "name": "momence-tech-task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm-run-all --parallel lint:**",
    "fix": "npm-run-all fix:eslint fix:prettier",
    "test": "npm-run-all --parallel test:**",
    "lint:prettier": "./node_modules/.bin/prettier --config .prettierrc.json --check \"./src/**/*.(ts|tsx|scss|css|sass)\"",
    "fix:prettier": "./node_modules/.bin/prettier --config .prettierrc.json --write \"./src/**/*.(ts|tsx|scss|css|sass)\"",
    "lint:ts": "./node_modules/.bin/tsc --noemit",
    "lint:eslint": "./node_modules/.bin/eslint src --ignore-path .gitignore --ext .ts,.tsx --cache --cache-location .cache/eslint/ --max-warnings 0",
    "fix:eslint": "./node_modules/.bin/eslint src/** --ignore-path .gitignore --ext .ts,.tsx --cache --cache-location .cache/eslint/ --fix",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run",
    "test:ci": "yarn cypress:run"
  },
  "author": "Jan Jadud <dev@gojadud.com>",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^2.5.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@westrem/currency.info": "0.1.2",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@westrem/eslint-config": "^1.0.0",
    "@westrem/prettier-config": "1.1.0",
    "cypress": "^12.8.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-compat": "^4.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
